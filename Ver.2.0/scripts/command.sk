        if arg-1 is "red":
            if {team.which.%arg-2%} is "red":
                play sound "block.note_block.banjo" to arg-2
                message "&b[システム] &rあなたはすでにそのチームに所属しています！" to arg-2
                stop
            if {team.which.%arg-2%} is "blue":
                reduce {qua.team.blue} by 1
                remove arg-2 from {team.blue::*}
                execute console command "scoreboard players reset %name of arg-2% blue"
            if {team.which.%arg-2%} is "yellow":
                reduce {qua.team.yellow} by 1
                remove arg-2 from {team.yellow::*}
                execute console command "scoreboard players reset %name of arg-2% yellow"
